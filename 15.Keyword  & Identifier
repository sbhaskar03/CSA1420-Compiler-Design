%{
#include <stdio.h>
%}

%%
"int"|"float"|"if"|"else"|"while"|"for"   { printf("Keyword: %s\n", yytext); }
[a-zA-Z_][a-zA-Z0-9_]*                    { printf("Identifier: %s\n", yytext); }
[ \t\n]                                  ;   /* ignore spaces, tabs, newlines */
%%

int yywrap(){ return 1; }

int main() {
    return yylex();
}
